                <property
                  dataType="System.String"
                  description="The SQL command to be executed."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">SELECT CAST(Visningsnamn AS NVARCHAR)AS Name
	,CAST(Organisationsnummer AS NVARCHAR(20))AS rik_Organisationsnummer
	,CAST(CASE WHEN avtalskund = 1 THEN '2' + Kundnummer ELSE Kundnummer END AS NVARCHAR(18)) AS rik_Idnummer
	,Epost AS emailaddress1
	--,NULL AS  emailaddress2
	,Tel1 AS address1_telephone1
	,Tel2 AS address1_telephone2
	,m.rik_Namn AS rik_marknadsomrade
	,ks.rik_Namn AS rik_kundsegment
	,ds.rik_Namn AS rik_delsegment
	,Adress1_Gata AS address1_line1 
	,Adress1_Co AS address1_line2
	,Adress1_Postnr AS address1_postalcode
	,Adress1_Ort AS address1_city
	,landskodAdr1 AS rik_address1_land
	,Adress2_Gata AS address2_line1 
	,Adress2_Co AS address2_line2
	,Adress2_Postnr AS address2_postalcode
	,Adress2_Ort AS address2_city
	,landskodAdr2 AS rik_address2_land
	,CRMguid AS rik_MigreringId
FROM STG.dbo.CRM_KUNDKONTO k
LEFT JOIN ovMarknadsomrade m
	ON m.sourceKodCRM = Marknadsomrade
LEFT JOIN ovKundsegment ks
	ON ks.sourceCRM = Kundsegment
LEFT JOIN ovDelsegment ds
	ON ds.sourceCRM = Delsegment</property>
