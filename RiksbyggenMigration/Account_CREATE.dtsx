            <component
              refId="Package\Load Account\STG_Account"
              componentClassID="Microsoft.OLEDBSource"
              contactInfo="OLE DB Source;Microsoft Corporation; Microsoft SQL Server; (C) Microsoft Corporation; All Rights Reserved; http://www.microsoft.com/sql/support;7"
              description="OLE DB Source"
              name="STG_Account"
              usesDispositions="true"
              version="7">
              <properties>
                <property
                  dataType="System.Int32"
                  description="The number of seconds before a command times out.  A value of 0 indicates an infinite time-out."
                  name="CommandTimeout">0</property>
                <property
                  dataType="System.String"
                  description="Specifies the name of the database object used to open a rowset."
                  name="OpenRowset">[dbo].[Account]</property>
                <property
                  dataType="System.String"
                  description="Specifies the variable that contains the name of the database object used to open a rowset."
                  name="OpenRowsetVariable"></property>
                <property
                  dataType="System.String"
                  description="The SQL command to be executed."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor, Microsoft.DataTransformationServices.Controls, Version=13.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91">SELECT  CRMguid AS AccountId
	,CAST(Visningsnamn AS NVARCHAR(160))AS Name
	,CAST(Organisationsnummer AS NVARCHAR(20))AS rik_Organisationsnummer
	,CAST(Kundnummer AS NVARCHAR(18)) AS rik_Idnummer
	,CASE WHEN KundansvarigAnvId IS NOT NULL THEN 8 ELSE NULL END AS owneridtype
	,Epost AS emailaddress1
	,Tel1 AS Telephone1
	,Tel2 AS Telephone2
	,CASE WHEN LEN([dbo].[fn_StripCharacters] (Tel1,'^0-9+')) &lt;= 4 THEN ''
			WHEN SUBSTRING([dbo].[fn_StripCharacters](Tel1,'^0-9+'),1,2) = '00' THEN '+' + SUBSTRING([dbo].[fn_StripCharacters](Tel1,'^0-9+'),3,LEN(Tel1)) 
			WHEN LEFT([dbo].[fn_StripCharacters] (Tel1,'^0-9+'),1) = '0' THEN '+46' + SUBSTRING([dbo].[fn_StripCharacters] (Tel1,'^0-9+'),2,LEN(Tel1))
			ELSE [dbo].[fn_StripCharacters](Tel1,'^0-9+') END AS rik_Telephone1Format
	,CASE WHEN LEN([dbo].[fn_StripCharacters] (Tel2,'^0-9+')) &lt;= 4 THEN ''
			WHEN SUBSTRING([dbo].[fn_StripCharacters](Tel2,'^0-9+'),1,2) = '00' THEN '+' + SUBSTRING([dbo].[fn_StripCharacters](Tel2,'^0-9+'),3,LEN(Tel2)) 
			WHEN LEFT([dbo].[fn_StripCharacters] (Tel2,'^0-9+'),1) = '0' THEN '+46' + SUBSTRING([dbo].[fn_StripCharacters] (Tel2,'^0-9+'),2,LEN(Tel2))
			ELSE [dbo].[fn_StripCharacters](Tel2,'^0-9+') END AS rik_Telephone2Format
	,COALESCE(m.rik_Kod,'999') AS rik_marknadsomrade
	,CAST(LEFT(Delsegment_IdNytt,2)AS NVARCHAR(2)) AS rik_kundsegment
	,CAST(Delsegment_IdNytt AS NVARCHAR(4)) AS rik_delsegment
	,Adress2_Gata AS address1_line1 
	,Adress2_Co AS address1_line2
	,Adress2_Postnr AS address1_postalcode
	,Adress2_Ort AS address1_city
	,landskodAdr2 AS rik_address1_land
	,Adress1_Gata AS address2_line1 
	,Adress1_Co AS address2_line2
	,Adress1_Postnr AS address2_postalcode
	,Adress1_Ort AS address2_city
	,landskodAdr1 AS rik_address2_land
	,CASE WHEN avtalskund  = 1  THEN 908400001   -- om avtalskund = statuscode = kvalificerade för avtal  bolag i PM (20 st)
		  WHEN kvalificerad = 1 THEN 908400000	 -- om kund i agresso = statuscode = kvalificerade för kund 
	 END AS statuscode													
	,0 AS statecode
	,CRMguid AS rik_MigreringId
FROM STG.dbo.CrmKundkonto k
LEFT JOIN ovMarknadsomrade m
	ON m.sourceKodCRM = Marknadsomrade</property>
