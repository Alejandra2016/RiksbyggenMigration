      <DTS:ObjectData>
        <SQLTask:SqlTaskData
          SQLTask:Connection="{68D9E5F8-BA63-4B16-A5EC-57708C0E20C9}"
          SQLTask:SqlStatementSource=";WITH Addres&#xA;AS (&#xA;&#x9; SELECT  *&#xA;&#x9;,ROW_NUMBER() OVER (PARTITION BY ParentId,AddressTypeCode  ORDER BY rb_FromDate DESC) AS RowNr&#xA;&#x9;FROM CustomerAddress&#xA;&#x9;)&#xA;UPDATE a &#xA;SET Address1_City = CASE WHEN ca.AddressTypeCode = 1 THEN ca.City END&#xA;, Address1_Country = CASE WHEN ca.AddressTypeCode = 1 THEN ca.Country END&#xA;, Address1_County = CASE WHEN ca.AddressTypeCode = 1 THEN ca.County END&#xA;, Address1_Line1 = CASE WHEN ca.AddressTypeCode = 1 THEN ca.Line1 END&#xA;, Address1_Line2 = CASE WHEN ca.AddressTypeCode = 1 THEN ca.Line2 END&#xA;, Address1_Line3 = CASE WHEN ca.AddressTypeCode = 1 THEN ca.Line3 END&#xA;, Address1_PostalCode = CASE WHEN ca.AddressTypeCode = 1 THEN ca.PostalCode END&#xA;, Address1_PostOfficeBox = CASE WHEN ca.AddressTypeCode = 1 THEN ca.PostOfficeBox END&#xA;, Address1_StateOrProvince = CASE WHEN ca.AddressTypeCode = 1 THEN ca.StateOrProvince END&#xA;, Address2_City = CASE WHEN caa.AddressTypeCode = 2 THEN caa.City  END&#xA;, Address2_Country = CASE WHEN caa.AddressTypeCode = 2 THEN ca.Country END&#xA;, Address2_County = CASE WHEN caa.AddressTypeCode = 2 THEN caa.County END&#xA;, Address2_Line1 = CASE WHEN caa.AddressTypeCode = 2 THEN caa.Line1 END&#xA;, Address2_Line2 = CASE WHEN caa.AddressTypeCode = 2 THEN caa.Line2 END&#xA;, Address2_Line3 = CASE WHEN caa.AddressTypeCode = 2 THEN caa.Line3 END&#xA;, Address2_PostalCode = CASE WHEN caa.AddressTypeCode = 2 THEN caa.PostalCode END&#xA;, Address2_PostOfficeBox = CASE WHEN caa.AddressTypeCode = 2 THEN caa.PostOfficeBox END&#xA;, Address2_StateOrProvince = CASE WHEN caa.AddressTypeCode = 2 THEN caa.StateOrProvince END&#xA;FROM account a&#xA;JOIN Addres ca&#xA;ON ca.ParentId = a.AccountId&#xA;&#x9;AND  (ca.AddressTypeCode = a.Address1_AddressTypeCode  )&#xA;JOIN Addres caa&#xA;ON caa.ParentId = a.AccountId&#xA;&#x9;AND  (caa.AddressTypeCode = a.Address2_AddressTypeCode )&#xA;WHERE ca.RowNr = 1&#xA;AND caa.RowNr = 1" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
      </DTS:ObjectData>
